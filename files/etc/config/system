
config system
	option hostname 'OpenWrt_192.168.171.10'
	option timezone 'UTC'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'

config rssid 'rssid_wlan0'
	option dev 'wlan0'
	option refresh '200000'
	option threshold '1'

config led 'led_wan'
	option name 'wan'
	option sysfs 'green:wan'
	option trigger 'switch0'
	option port_mask '0x01'

config led 'led_wifi_low'
	option sysfs 'green:wifi-low'
	option trigger 'rssi'
	option iface 'rssid_wlan0'
	option minq '1'
	option maxq '49'
	option offset '0'
	option factor '1'

config led 'led_wifi_med'
	option sysfs 'green:wifi-med'
	option trigger 'rssi'
	option iface 'rssid_wlan0'
	option minq '50'
	option maxq '84'
	option offset '0'
	option factor '1'

config led 'led_wifi_high'
	option sysfs 'green:wifi-high'
	option trigger 'rssi'
	option iface 'rssid_wlan0'
	option minq '85'
	option maxq '100'
	option offset '0'
	option factor '1'

config led 'led_wifi_led'
	option name 'wifi'
	option sysfs 'green:wifi'
	option trigger 'netdev'
	option mode 'link tx rx'
	option dev 'wlan0'

